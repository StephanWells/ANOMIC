<Window x:Class="AnnotationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnnotationTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="791*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnBrowse" Content="Browse" HorizontalAlignment="Left" Margin="20,200,0,199" VerticalAlignment="Center" Width="75" RenderTransformOrigin="0.259,4.613" Click="btnBrowse_Click"/>
        <TextBlock x:Name="blkParseOutput" HorizontalAlignment="Left" TextWrapping="Wrap" Text="MIDI output goes here." VerticalAlignment="Top" Height="500" Width="300" Margin="150,30,0,0"/>

    </Grid>
</Window>
