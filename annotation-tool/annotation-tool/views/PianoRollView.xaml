<UserControl x:Class="AnnotationTool.views.PianoRollView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AnnotationTool.views"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="1000">

    <UserControl.Resources>
        <ResourceDictionary Source="../ResourceDict.xaml"/>
    </UserControl.Resources>

    <Grid x:Name="MainGrid" Background="{StaticResource GridDarkColour}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="125"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Separator Grid.Row="0" Grid.RowSpan="2" Style="{DynamicResource VerticalSeparatorStyle}" Panel.ZIndex="6" Margin="6,0,0,0" HorizontalAlignment="Right"/>
        <Separator Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Top" Panel.ZIndex="6"/>

        <Grid Grid.Row="0" Grid.Column="0">
            <TextBlock x:Name="txtPatternsHeader" FontSize="16" Text="PATTERNS" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Margin="6,0,0,0"></TextBlock>
        </Grid>

        <Grid Grid.Row="0" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="96"/>
                <ColumnDefinition Width="4*"/>
            </Grid.ColumnDefinitions>

            <TextBlock x:Name="txtFileName" Grid.Column="0" Margin="5,1,0,0"></TextBlock>
            <TextBlock x:Name="txtBPM" Grid.Column="2" Margin="0,1,25,0" HorizontalAlignment="Right"></TextBlock>

            <Grid Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="32"/>
                    <ColumnDefinition Width="32"/>
                    <ColumnDefinition Width="32"/>
                </Grid.ColumnDefinitions>

                <Button x:Name="btnPlay" Grid.Column="0" FontFamily="Segoe MDL2 Assets" Content="&#xF5B0;" FontSize="16" Margin="0,0,0,0" Click="Play_Click"/>
                <Button x:Name="btnPause" Grid.Column="1" FontFamily="Segoe MDL2 Assets" Content="&#xE769;" FontSize="18" Margin="0,0,0,0" Click="Pause_Click"/>
                <Button x:Name="btnStop" Grid.Column="2" FontFamily="Segoe MDL2 Assets" Content="&#xE747;" FontSize="12" Margin="0,0,0,0" Click="Stop_Click"/>
            </Grid>
        </Grid>

        <ScrollViewer Margin="0,0,0,0" Grid.Row="1">
            <Grid Grid.Row="1" Grid.Column="0">
                <ItemsControl x:Name="itmPatternsView" Background="Transparent"/>
                <Button x:Name="btnAddPattern" HorizontalAlignment="Stretch" Height="50" VerticalAlignment="Top" Margin="0,0,0,0" Click="AddPattern_Click">
                    <TextBlock FontSize="30" FontFamily="Segoe MDL2 Assets" TextAlignment="Center" Height="30" Width="30" Margin="0,0,0,0">&#xE710;</TextBlock>
                </Button>
            </Grid>
